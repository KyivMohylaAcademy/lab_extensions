{
  "manifest_version": 3,
  "name": "Crypto wallet",
  "version": "1.0.0",
  "description": "A simple Chrome crypto wallet.",
  "icons": {
    "16": "Eo_circle_blue_192.png",
    "48": "Eo_circle_blue_192.png",
    "128": "Eo_circle_blue_192.png"
  },
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ]
}